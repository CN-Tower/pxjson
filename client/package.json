{
  "name": "pxjson",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "node ./node_modules/@angular/cli/bin/ng",
    "start": "node ./node_modules/@angular/cli/bin/ng serve -pc proxy.conf.json -o",
    "build": "node ./node_modules/@angular/cli/bin/ng build --base-href .",
    "electron": "npm run build-electron && electron dist",
    "build-electron": "npm run build && node build/electron.conf.js",
    "update": "npm run build && node ./tools/update.js",
    "package": "npm run electron && electron-packager ./dist PxJsonTool --platform=win32 --out ./build/package --verson=1.0.1 --overwrite --icon=./assets/imgs/favicon.ico",
    "test": "node ./node_modules/@angular/cli/bin/ng test",
    "lint": "node ./node_modules/@angular/cli/bin/ng lint",
    "e2e": "node ./node_modules/@angular/cli/bin/ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.2",
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/router": "^4.0.2",
    "bootstrap": "^3.3.7",
    "core-js": "2.4.1",
    "datatables.net": "^1.10.13",
    "echarts": "^3.4.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "jquery-ui-dist": "^1.12.1",
    "jsplumb": "^2.3.2",
    "ng2-bootstrap": "1.6.3",
    "ng2-toastr": "^4.0.1",
    "ng2-translate": "5.0.0",
    "portfinder": "1.0.13",
    "rxjs": "^5.4.2",
    "tinymce": "4.5.6",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.8"
  },
  "devDependencies": {
    "@angular/cli": "1.0.1",
    "@angular/compiler-cli": "^4.0.2",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.13",
    "codelyzer": "3.0.0",
    "electron": "^1.6.11",
    "electron-packager": "^8.7.2",
    "ember-cli": "2.12.2",
    "jasmine-core": "2.6.0",
    "jasmine-spec-reporter": "^4.0.0",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "0.6.0",
    "protractor": "5.1.1",
    "ts-node": "3.0.2",
    "tslint": "5.1.0",
    "typescript": "^2.4.1"
  }
}
